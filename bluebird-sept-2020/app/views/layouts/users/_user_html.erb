<%# no instance varialbes in partials  %>

<% if action == 'show' %>
    <a href="<%= users_url %>%">Users Index Link</a>
<% else %>
    <a href="<%= user_url(user) %>%">Users Show Link</a>
<% end %>


<ul class="user-attributes">
    <li><%= user.username %></li>
    <li><%= user.age %></li>
    <li><%= user.email %></li>
</ul>